@using ManejoEmpleados.Models
@model Departamentos

@{
    ViewData["Title"] = "Editar departamento";

}

<partial name="DashBoardSideBar" model="DashboardMenu.Departamentos"></partial>


git <div>




    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <form class="formGeneral" asp-controller="Departamentos" asp-action="Editar" method="post">
        <input type="hidden" asp-for="Id" />
        
        <h2><span class="Span-R">E</span>ditar</h2>
        <div class="mb-3">


            <label for="Nombre">Nombre Completo</label>
            <input class="inputs" asp-for="Nombre" type="text" />
            <span asp-validation-for="Nombre" class="text-danger"></span>
        </div>


       
        <div class="mb-3">
            <button class="btnRegistro" type="submit">Guardar</button>
            <a asp-controller="Departamentos" asp-action="Lista" class="btnLogin">Volver</a>
        </div>
    </form>


</div>










